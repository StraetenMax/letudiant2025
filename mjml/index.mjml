        <mj-text mj-class="T" padding-bottom="0"><span style="font-weight:700">AEPE</span> Accompagnant(e) Éducatif Petite Enfance</mj-text>
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding="0">
      <mj-column width="50%" vertical-align="middle">
        <mj-image padding="0" border="none" fluid-on-mobile="true" width="481px" src="https://i.ibb.co/8DhSw8c/02supveto14122024.jpg" alt=""></mj-image>
      </mj-column>
      <mj-column width="50%" vertical-align="middle">
        <mj-text mj-class="T+2" padding-bottom="0">Supveto</mj-text>
        <mj-text mj-class="T" padding-bottom="0"><span style="font-weight:700">ASV</span> Auxiliaire de Service Vétérinaire</mj-text>
        <mj-text mj-class="T" padding-bottom="0"><span style="font-weight:700">Bachelor</span> Chargé(e) de gestion et management en structure animalière</mj-text>
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding="20px 50px"> 
